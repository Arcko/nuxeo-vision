<!-- See documentation at http://doc.nuxeo.com/x/HgIz -->
<package type="addon" name="nuxeo-vision" version="@VERSION@">
  <title>Nuxeo Vision Package</title>
  <description>
    <p>This plugin provides a wrapper for Computer Vision Services.
      Currently it supports the Google Vision API.</p>
    <p>Requires Nuxeo DAM.</p>
  </description>
  <!-- <home-page>http://doc.nuxeo.com/</home-page> -->
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <!-- <hotreload-support>false</hotreload-support> -->
  <!-- <require-terms-and-conditions-acceptance>false</require-terms-and-conditions-acceptance> -->
  <nuxeo-validation>none</nuxeo-validation>
  <!-- <nuxeo-validation>inprocess</nuxeo-validation> -->
  <!-- <nuxeo-validation>primary_validation</nuxeo-validation> -->
  <!-- <nuxeo-validation>nuxeo_certified</nuxeo-validation> -->
  <!-- <production-state>proto</production-state> -->
  <production-state>testing</production-state>
  <!-- <production-state>production_ready</production-state> -->
  <supported>false</supported>
  <platforms>
    <platform>cap-@DISTRIB_VERSION@</platform>
    <platform>cap-@DISTRIB_VERSION@-HF*</platform>
  </platforms>
  <!-- Dependencies on other packages -->
  <dependencies>
    <package>nuxeo-dam</package>
  </dependencies>
  <!-- Conflicts with other packages -->
  <!-- <conflicts> -->
  <!-- <package>a-conflicting-package:1.0.0:1.0.0</package> -->
  <!-- </conflicts> -->
  <!-- Provides features of other packages -->
  <!-- <provides> -->
  <!-- <package>an-embedded-package:1.0.0:1.0.0</package> -->
  <!-- </provides> -->
  <license>Apache License, Version 2.0</license>
  <license-url>http://www.apache.org/licenses/LICENSE-2.0</license-url>
  <!-- Visibility: PRIVATE | MARKETPLACE | DEV | PUBLIC -->
  <visibility>PUBLIC</visibility>
</package>
