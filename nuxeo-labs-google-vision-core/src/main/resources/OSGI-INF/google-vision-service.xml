<?xml version="1.0"?>
<component name="org.nuxeo.labs.vision.core.service" version="1.0">

  <implementation class="org.nuxeo.labs.vision.core.service.GoogleVisionImpl" />

  <service>
    <provide interface="org.nuxeo.labs.vision.core.service.GoogleVision" />
  </service>

  <extension-point name="configuration">
    <documentation></documentation>
    <object class="org.nuxeo.labs.vision.core.service.GoogleVisionDescriptor" />
  </extension-point>


  <extension target="org.nuxeo.labs.vision.core.service" point="configuration">
    <configuration>
      <appName>Nuxeo</appName>
      <credentialFilePath>${org.nuxeo.labs.google.credential:=}</credentialFilePath>
    </configuration>
  </extension>

</component>
